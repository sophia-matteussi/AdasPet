
@{
    ViewData["Title"] = "BrinquedosRoedores";
}

<h1>BrinquedosRoedores</h1>


<div>
    @foreach (var item in AdasPet.Controllers.FrontSupport.Lista("Brinquedo", "Roedores"))
    {
        <div class="list-group">
            <label>@item.Nome</label>
            <label>@item.Preco</label>
            <label>@item.Categoria</label>
            <label>@item.TipoDeAnimal</label>
            <label>@item.QtdEmEstoque</label>
            @if (item.PrecisaDeCarro)
            {
                <label>Sim</label>
            }
            else
            {
                <label>Não</label>
            }
            <form action="/" method="post">
            </form>
            <br />
        </div>
    }
</div>